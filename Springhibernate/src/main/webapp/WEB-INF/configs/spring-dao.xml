<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean name="addressDAO" class="quyet.demo.spring.dao.impl.AddressDAOImpl">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>

	<bean name="storeDAO" class="quyet.demo.spring.dao.impl.StoreDAOImpl">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>

	<bean name="categoryDAO" class="quyet.demo.spring.dao.impl.CategoryDAOImpl">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>

	<bean name="productDAO" class="quyet.demo.spring.dao.impl.ProductDAOImpl">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>
</beans>
